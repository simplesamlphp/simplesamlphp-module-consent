{
    "name": "simplesamlphp/simplesamlphp-module-consent",
    "description": "A module that will ask for user consent before releasing attributes",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "consent"],
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Olav Morken",
            "email": "lavmrk@gmail.com"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "simplesamlphp/composer-module-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "simplesamlphp/composer-xmlprovider-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\consent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",

        "simplesamlphp/assert": "^1.9",
        "simplesamlphp/composer-module-installer": "^1.5",
        "simplesamlphp/saml2": "^6.0",

        "simplesamlphp/simplesamlphp": "^2.5@dev",
        "symfony/http-foundation": "^7.4"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.11"
    },
    "support": {
        "issues": "https://github.com/simplesamlphp/simplesamlphp-module-consent/issues",
        "source": "https://github.com/simplesamlphp/simplesamlphp-module-consent"
    }
}
